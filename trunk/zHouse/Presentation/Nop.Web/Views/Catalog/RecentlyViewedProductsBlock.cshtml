@model IList<ProductOverviewModel>
@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{
    
      <div class="pinkNarrowTitleSmall" style="margin-bottom:10px">Bạn vừa xem</div>
    foreach (var product in Model)
    {
             @Html.Partial("_ProductBoxSmall",product)

    }

                <div class="pinkBgRightBottom">
                    <a style="font-size:32px" href="@Url.RouteUrl("AddProductToCompare", new {productIds=Model.Select(x=>x.Id.ToString()).Aggregate((a,b)=>string.Format("{0},{1}",a,b)) })" >SO SÁNH</a><br>
                    CÁC BĐS VỪA XEM
                </div>

}