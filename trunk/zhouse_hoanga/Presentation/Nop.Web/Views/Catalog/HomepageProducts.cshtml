@model IList<ProductOverviewModel>
@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{
    <div class="wrapper">
        <div class="intro-product">            
            <div class="title"><h2 class="block-title">@ViewBag.TitleString</h2><a href="@ViewBag.All">Xem tất cả</a></div>
            <div id="list-product-home">                
                    @foreach(var item in Model)
                    {
                            @Html.Partial("_ProductBox",item)
                    }
            </div>
        </div>
          <div class="clearBoth"></div>           
    </div>    
}