@model List<PlanX.Web.Models.ClickBay.TicketConcessionModel>
@if (Model.Count > 0)
{
    <div class="wrapper-body">
        <div class="wrapper-content">
            <section class="box-line">
                <div class="nv-title">Nhượng vé - Clickbay cùng bạn tiết kiệm</div><div class="line"><p class="boder-dashed line"></p></div>
            </section>            
            <section class="TicketConcession">
                <p class="center">
                    <span class="home-title">Bạn đã mua vé nhưng không thể bay</span>
                    <a class="postitem" name="postitem" href="@Url.RouteUrl("TicketConcession")">Đăng tin nhượng vé</a>
                </p>
            </section>
            <section class="TicketConcession">
                <div>
                    @Html.Partial("_ListTicketConcession", Model)
                </div>
            </section>
        </div>
    </div>}