@model List<PlanX.Web.Models.News.NewsTagModel>
@if(Model.Count>0){
<div class="tags_box">
    <div class="left">
        <h2 class="color_white"><span>TAG</span><span class="small">S</span></h2>
    </div>
    <div class="right">
        <div>
            @foreach (var item in Model) { 
            <p><a href="@Url.RouteUrl("NewsItemsByTag", new { SeName = item.SeName, newsTagId = item.Id })">@item.Name</a></p>}            
        </div>
    </div>
</div>}